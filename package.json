{
  "name": "buriReact",
  "version": "0.1.0",
  "description": "Starter kit for creating apps with React and Redux",
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "eslint src/ tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint && npm run test && npm run clean-dist",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "jest --config config/jest.json",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --config config/jest.json --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Abhinav Mishra",
  "license": "MIT",
  "dependencies": {
    "object-assign": "4.1.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@nimbl3/eslint-config-nimbl3": "2.0.1",
    "autoprefixer": "7.2.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "22.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.19.0",
    "chalk": "2.3.0",
    "concurrently": "3.5.1",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.0",
    "css-loader": "0.28.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.13.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "3.1.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.0.3",
    "jest-cli": "22.0.3",
    "json-loader": "0.5.7",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "mockdate": "2.0.2",
    "node-sass": "4.7.2",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.9",
    "prompt": "1.0.0",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "16.2.0",
    "redux-immutable-state-invariant": "2.1.0",
    "replace": "0.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.1",
    "webpack-dev-middleware": "2.0.2",
    "webpack-hot-middleware": "2.21.0",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [
